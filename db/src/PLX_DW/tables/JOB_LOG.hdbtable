COLUMN TABLE "JOB_LOG"(
	"NAME" NVARCHAR(512) NOT NULL,
	"ID" BIGINT  NOT NULL,
	"PLANNED_TIME" LONGDATE  NOT NULL,
	"STATUS" VARCHAR(16) NOT NULL,
	"ERROR_MESSAGE" NVARCHAR(2048),
	"STARTED_AT" LONGDATE ,
	"FINISHED_AT" LONGDATE,
	"HOST" NVARCHAR(256),
	"PORT" VARCHAR(5),
	"ACTION" NVARCHAR(512),
	"USER" NVARCHAR(256),
	"LOCALE" VARCHAR(64),
	"THREAD_ID" BIGINT,
	PRIMARY KEY (
			"NAME",
			"ID",
			"PLANNED_TIME"
		     )

) UNLOAD PRIORITY 0 AUTO MERGE;
