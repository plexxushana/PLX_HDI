VIEW "DAILY_LOAD_REPORT" ( "TASK_NAME", "START_TIME_LOCAL_TZ", "END_TIME_LOCAL_TZ", "DURATION", "HOURS", "MINUTES", "SECONDS", "STATUS", "PROCESSED_RECORDS", "TOTAL_PROGRESS_PERCENT" ) AS SELECT                "TASK_NAME"                , utctolocal("START_TIME", 'EST') as start_time_local_tz                 , utctolocal("END_TIME", 'EST') as end_time_local_tz                , "DURATION"                , to_int(("DURATION" / 1000000) / 3600) as "HOURS"                , to_int(mod(("DURATION" / 1000000), 3600) / 60) as "MINUTES"                , MOD(MOD(MOD(("DURATION" / 1000000) , 3600) , 60), 60) as "SECONDS"                , "STATUS"                , "PROCESSED_RECORDS"                , "TOTAL_PROGRESS_PERCENT"FROM "SYS::M_TASKS"where to_date(start_time) = current_date WITH READ ONLY