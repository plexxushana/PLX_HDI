      <calculatedAttribute id="Z_INV_PRICE" hidden="false" order="50" semanticType="empty" attributeHierarchyActive="false" displayAttribute="false">
        <descriptions defaultDescription="Invoice Price">
          <comment text="if( isnull(&quot;Z_INV_LAMT&quot;) or  isnull(&quot;Z_INVOICE_QTY&quot;) or  &quot;Z_INV_LAMT&quot; = 0 or  &quot;Z_INVOICE_QTY&quot; = 0, 0.0, &quot;Z_INV_LAMT&quot; / &quot;Z_INVOICE_QTY&quot; )"/>
        </descriptions>
        <keyCalculation datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="13" scale="3">
          <formula>if( isnull(&quot;Z_INV_LAMT&quot;) or  isnull(&quot;Z_INVOICE_QTY&quot;) or  &quot;Z_INV_LAMT&quot; = 0 or  &quot;Z_INVOICE_QTY&quot; = 0, 0.0, &quot;Z_INV_LAMT&quot; / &quot;Z_INVOICE_QTY&quot; )</formula>
        </keyCalculation>
      </calculatedAttribute>
      <calculatedAttribute id="Z_PRICE_DIFF_PCT" hidden="false" order="93" semanticType="empty" attributeHierarchyActive="false" displayAttribute="false">
        <descriptions defaultDescription="Z_PRICE_DIFF_PCT"/>
        <localVariable>#PRICE_DIFF_PCT</localVariable>
        <keyCalculation datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="3" scale="2">
          <formula>if(isnull(&quot;PREVIOUS_PRICE_EA&quot;) OR &quot;PREVIOUS_PRICE_EA&quot; = 0, 1, (&quot;PREVIOUS_PRICE_EA&quot; - &quot;Z_INV_BUOM_PRICE&quot;) / &quot;PREVIOUS_PRICE_EA&quot;) * 100&#xD;
</formula>
        </keyCalculation>
      </calculatedAttribute>
    </calculatedAttributes>
