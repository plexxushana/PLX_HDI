      <measure id="Z_SPEND_IB" hidden="false" order="49" aggregationType="sum" measureType="simple" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="13" scale="3">
        <descriptions defaultDescription="Spend (Invoice Based)">
          <comment text="&quot;Z_INV_BUOM_PRICE&quot;* &quot;Z_INV_BQTY&quot;"/>
        </descriptions>
        <formula>&quot;Z_INV_BUOM_PRICE&quot;* &quot;Z_INV_BQTY&quot;</formula>
      </measure>
      <measure id="Z_SAV_IB" hidden="false" order="79" aggregationType="sum" measureType="simple" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="13" scale="3">
        <descriptions defaultDescription="Savings (Invoice Based)">
          <comment text="if(     isnull(&quot;PREVIOUS_PRICE_EA&quot;),     0.0,     (&quot;PREVIOUS_PRICE_EA&quot; -           (if( isnull(&quot;Z_INV_LAMT&quot;) or  isnull(&quot;Z_INV_BQTY&quot;) or &quot;Z_INV_LAMT&quot; = 0 or &quot;Z_INV_BQTY&quot; = 0, 0.0, &quot;Z_INV_LAMT&quot; / &quot;Z_INV_BQTY&quot; ))          ) * &quot;Z_INV_BQTY&quot;    )"/>
        </descriptions>
        <formula>if(     isnull(&quot;PREVIOUS_PRICE_EA&quot;),     0.0,     (&quot;PREVIOUS_PRICE_EA&quot; -           (if( isnull(&quot;Z_INV_LAMT&quot;) or  isnull(&quot;Z_INV_BQTY&quot;) or &quot;Z_INV_LAMT&quot; = 0 or &quot;Z_INV_BQTY&quot; = 0, 0.0, &quot;Z_INV_LAMT&quot; / &quot;Z_INV_BQTY&quot; ))          ) * &quot;Z_INV_BQTY&quot;    )</formula>
      </measure>
      <measure id="Z_INV_BUOM_PRICE" hidden="false" order="80" aggregationType="sum" measureType="simple" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="13" scale="3">
        <descriptions defaultDescription="Invoice Base Unit of Measure Price">
          <comment text="if( isnull(&quot;Z_INV_LAMT&quot;) or  isnull(&quot;Z_INV_BQTY&quot;) or &quot;Z_INV_LAMT&quot; = 0 or &quot;Z_INV_BQTY&quot; = 0, 0.0, &quot;Z_INV_LAMT&quot; / &quot;Z_INV_BQTY&quot; )"/>
        </descriptions>
        <formula>if( isnull(&quot;Z_INV_LAMT&quot;) or  isnull(&quot;Z_INV_BQTY&quot;) or &quot;Z_INV_LAMT&quot; = 0 or &quot;Z_INV_BQTY&quot; = 0, 0.0, &quot;Z_INV_LAMT&quot; / &quot;Z_INV_BQTY&quot; )</formula>
      </measure>
      <measure id="Z_SAV_IUOM" hidden="false" order="81" aggregationType="sum" measureType="simple" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="13" scale="3">
        <descriptions defaultDescription="Invoice Unit of Measure Price">
          <comment text="if(     isnull(&quot;PREVIOUS_PRICE_EA&quot;),     0.0,     (&quot;PREVIOUS_PRICE_EA&quot; - &quot;Z_INV_PRICE&quot;) * &quot;Z_INVOICE_QTY&quot;    )"/>
        </descriptions>
        <formula>if(     isnull(&quot;PREVIOUS_PRICE_EA&quot;),     0.0,     (&quot;PREVIOUS_PRICE_EA&quot; - &quot;Z_INV_PRICE&quot;) * &quot;Z_INVOICE_QTY&quot;    )</formula>
      </measure>
    </calculatedMeasures>
