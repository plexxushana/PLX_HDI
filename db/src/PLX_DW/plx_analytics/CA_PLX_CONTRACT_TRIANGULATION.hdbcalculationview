    <calculatedViewAttributes>
        <calculatedViewAttribute datatype="DECIMAL" id="ZPV_DCCW_DCREP" length="34" scale="2" expressionLanguage="COLUMN_ENGINE">
          <formula>         
IF(&quot;NETPR&quot;&gt; 0 ,IF(&quot;ZCONV1&quot;&gt; 0 ,IF(&quot;ZNETPR1&quot;&gt; 0 ,IF(&quot;ZCONV3&quot;&gt; 0, ((&quot;NETPR&quot; / &quot;ZCONV1&quot; ) -  (&quot;ZNETPR1&quot;/ &quot;ZCONV3&quot;)     ),0),0),0),0)

            </formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="ZTPV_DCCW_DCREP" length="34" scale="2" expressionLanguage="COLUMN_ENGINE">
          <formula>     

IF(&quot;NETPR&quot;&gt; 0 ,IF(&quot;ZCONV1&quot;&gt; 0 ,IF(&quot;ZNETPR1&quot;&gt; 0 ,IF(&quot;ZCONV3&quot;&gt; 0,IF(&quot;FKIMG&quot;&gt; 0, (((&quot;NETPR&quot; / &quot;ZCONV1&quot; ) -  (&quot;ZNETPR1&quot;/ &quot;ZCONV3&quot;) )*&quot;FKIMG&quot;   ),0),0),0),0),0)

            </formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="ZPV_DCCW_BD" length="34" scale="2" expressionLanguage="COLUMN_ENGINE">
          <formula>IF(&quot;NETPR&quot;&gt; 0 ,IF(&quot;ZCONV1&quot;&gt; 0 ,IF(&quot;NETWR&quot;&gt; 0 ,IF(&quot;FKIMG&quot;&gt; 0, ((&quot;NETPR&quot; / &quot;ZCONV1&quot;) - (&quot;NETWR&quot; /  &quot;FKIMG&quot;)),0),0),0),0)

            
</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="ZTPV_DCCW_BD" length="34" scale="2" expressionLanguage="COLUMN_ENGINE">
          <formula>IF(&quot;NETPR&quot;&gt; 0 ,IF(&quot;ZCONV1&quot;&gt; 0 ,IF(&quot;NETWR&quot;&gt; 0 ,IF(&quot;FKIMG&quot;&gt; 0, (((&quot;NETPR&quot; / &quot;ZCONV1&quot;) - (&quot;NETWR&quot; /  &quot;FKIMG&quot;))*&quot;FKIMG&quot;   ),0),0),0),0)
</formula>
        </calculatedViewAttribute>
      </calculatedViewAttributes>
      <input node="Join_1"/>
    </calculationView>
