  <calculationView xsi:type="Calculation:AggregationView" id="Aggregation_1">
    <descriptions/>
    <viewAttributes>
        <viewAttribute aggregationType="sum" id="ChequeLineCount"/>
        <viewAttribute aggregationType="sum" id="PaymentsLineCount"/>
        <viewAttribute aggregationType="sum" id="DcoumentCount_ZB"/>
        <viewAttribute aggregationType="sum" id="DocumentCount_KR_RE_ZA_ZB"/>
