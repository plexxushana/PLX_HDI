        <viewAttribute aggregationType="sum" id="EDILinesCount"/>
        <viewAttribute aggregationType="max" id="TARGET"/>
        <viewAttribute aggregationType="max" id="THRESHOLD"/>
        <viewAttribute id="MEMBER"/>
        <viewAttribute id="ZFISCAL_YEAR"/>
        <viewAttribute id="ZFISCAL_PERIOD"/>
        <viewAttribute id="BUKRS"/>
        <viewAttribute id="BUTXT"/>
        <viewAttribute id="ZFISCAL_QUARTER"/>
        <viewAttribute id="ZFISCAL_YEAR_QUARTER"/>
        <viewAttribute id="ZFISCAL_MONTH"/>
        <viewAttribute aggregationType="sum" id="Req_LineCount_NoPLX"/>
        <viewAttribute aggregationType="sum" id="Req_LineCount_Total"/>
        <viewAttribute aggregationType="sum" id="eReq_LineCount"/>
        <viewAttribute aggregationType="sum" id="Req_HeaderCount_Total"/>
        <viewAttribute aggregationType="sum" id="DocumentCount_ZBPLX"/>
        <viewAttribute aggregationType="sum" id="Non_ePayments_LineCount"/>
        <viewAttribute aggregationType="sum" id="eReq_HeaderCount"/>
        <viewAttribute aggregationType="sum" id="Req_HeaderCount_NoPLX"/>
        <viewAttribute aggregationType="max" id="MULTIPLY_FACTOR"/>
        <viewAttribute aggregationType="max" id="DENOMINATOR"/>
        <viewAttribute aggregationType="sum" id="PO_DOCUMENT_COUNT"/>
        <viewAttribute aggregationType="sum" id="CC_ELECTRONIC_PO_HDR_CNTR"/>
      </viewAttributes>
    <calculatedViewAttributes>
        <calculatedViewAttribute datatype="DECIMAL" id="Percentage_eBusiness" length="13" scale="10" expressionLanguage="COLUMN_ENGINE">
          <formula>(&quot;Percentage_ePayments&quot;+&quot;Percentage_eInvoices&quot;+&quot;Percentage_ePO&quot;+&quot;Percentage_eReq&quot;*&quot;MULTIPLY_FACTOR&quot;)/&quot;DENOMINATOR&quot;</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="Percentage_ePayments" length="13" scale="10" expressionLanguage="COLUMN_ENGINE">
          <formula>if(&quot;PaymentsLineCount&quot; &gt; 0, (&quot;PaymentsLineCount&quot;-&quot;Non_ePayments_LineCount&quot;)/&quot;PaymentsLineCount&quot;, 0)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="Percentage_eInvoices" length="13" scale="10" expressionLanguage="COLUMN_ENGINE">
          <formula>if ( &quot;DocumentCount_KR_RE_ZA_ZB&quot; &gt; 0, (&quot;DcoumentCount_ZB&quot;+&quot;DocumentCount_PLX&quot; )/ &quot;DocumentCount_KR_RE_ZA_ZB&quot;, 0 )</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="Percentage_ePO" length="13" scale="10" expressionLanguage="COLUMN_ENGINE">
          <formula>if(&quot;PO_DOCUMENT_COUNT&quot; &gt; 0 , &quot;CC_ELECTRONIC_PO_HDR_CNTR&quot;/&quot;PO_DOCUMENT_COUNT&quot;,0)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="Percentage_eReq" length="13" scale="10" expressionLanguage="COLUMN_ENGINE">
          <formula>if(&quot;eReq_HeaderCount&quot;&gt; 0 , &quot;eReq_HeaderCount&quot;/&quot;Req_HeaderCount_NoPLX&quot;, Null)</formula>
        </calculatedViewAttribute>
      </calculatedViewAttributes>
    <restrictedViewAttributes/>
      <input node="Projection_7"/>
    </calculationView>
