        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="NVARCHAR" id="Z_UTILIZATION" length="1" expressionLanguage="COLUMN_ENGINE">
          <formula>if(TRIM(&quot;KOSTL&quot;) !='' AND TRIM(&quot;BWART&quot;)!='161'  AND TRIM(&quot;BWART&quot;)!='162'  AND TRIM(&quot;BWART&quot;)!='202'  AND TRIM(&quot;BWART&quot;)!='601' AND TRIM(&quot;BWART&quot;)!='645' AND TRIM(&quot;BWART&quot;)!='653' AND TRIM(&quot;BWART&quot;)!='675'  AND (NOT (MATCH(TRIM(&quot;BWART&quot;), '3*')))  AND (NOT (MATCH(TRIM(&quot;BWART&quot;), '7*'))), 'X','')</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="NVARCHAR" id="Z_SUPPLY" length="1" expressionLanguage="COLUMN_ENGINE">
          <formula>if(TRIM(&quot;KOSTL&quot;)='' AND TRIM(&quot;BWART&quot;)!='161'  AND TRIM(&quot;BWART&quot;)!='162'  AND TRIM(&quot;BWART&quot;)!='202'  AND TRIM(&quot;BWART&quot;)!='601' AND TRIM(&quot;BWART&quot;)!='645' AND TRIM(&quot;BWART&quot;)!='653' AND TRIM(&quot;BWART&quot;)!='675'  AND (NOT (MATCH(TRIM(&quot;BWART&quot;), '3*')))  AND (NOT (MATCH(TRIM(&quot;BWART&quot;), '7*'))), 'X','')</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="NVARCHAR" id="Z_ADJUSTMENTS" length="1" expressionLanguage="COLUMN_ENGINE">
          <formula>IF(MATCH(TRIM(&quot;BWART&quot;),  '7*') , 'X', '')  </formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="NVARCHAR" id="Z_RETURN_TO_STORES" length="1" expressionLanguage="COLUMN_ENGINE">
          <formula>IF(MATCH(TRIM(&quot;KOSTL&quot;), '' )  AND IN(&quot;BWART&quot; , '161', '162', '202' ) , 'X', '')</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="NVARCHAR" id="Z_TRANSFER" length="25" expressionLanguage="COLUMN_ENGINE">
          <formula>IF( MATCH(TRIM(&quot;BWART&quot;), '3*'), 'X','')</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="NVARCHAR" id="Z_DC_SHIPMENT" length="1" expressionLanguage="COLUMN_ENGINE">
          <formula>IF(TRIM(&quot;BWART&quot;)='601' OR TRIM(&quot;BWART&quot;)='645', 'X', '')</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="NVARCHAR" id="Z_RETURN_DC" length="1" expressionLanguage="COLUMN_ENGINE">
          <formula>IF(TRIM(&quot;BWART&quot;)='653' OR TRIM(&quot;BWART&quot;)='675' , 'X', '')</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="NVARCHAR" id="Z_ACTIVITY" length="25" expressionLanguage="COLUMN_ENGINE">
          <formula>IF(&quot;Z_UTILIZATION&quot;='X', 'Utilization', if(&quot;Z_SUPPLY&quot;='X', 'SUPPLY', IF(&quot;Z_ADJUSTMENTS&quot;='X', 'Adjustments', IF(&quot;Z_RETURN_TO_STORES&quot;='X', 'Return to Stores', IF(&quot;Z_TRANSFER&quot;='X','Transfer', IF(&quot;Z_DC_SHIPMENT&quot;='X','DC Shipment', IF(&quot;Z_RETURN_DC&quot;='X','Return to DC','Unclassified')))))))</formula>
        </calculatedViewAttribute>
      </calculatedViewAttributes>
