          <formula>DAYDATE(&quot;BUDAT&quot;)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="INTEGER" id="WEEK_DA" expressionLanguage="SQL">
          <formula>weekday(&quot;DT_BUDAT&quot;) + 1</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="NVARCHAR" id="START_DT_WEEK" length="8" expressionLanguage="COLUMN_ENGINE">
          <formula>IF(&quot;WEEK_DA&quot;=7, adddays(DAYDATE(&quot;BUDAT&quot;), (-1)*&quot;WEEK_DA&quot;+7), adddays(DAYDATE(&quot;BUDAT&quot;), (-1)*&quot;WEEK_DA&quot;))</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="NVARCHAR" id="END_DT_WEEK" length="20" expressionLanguage="COLUMN_ENGINE">
          <formula>IF(&quot;WEEK_DA&quot;=7, adddays(DAYDATE(&quot;BUDAT&quot;), ( 6-&quot;WEEK_DA&quot;))+7 , adddays(DAYDATE(&quot;BUDAT&quot;), ( 6-&quot;WEEK_DA&quot;)))</formula>
        </calculatedViewAttribute>
      </calculatedViewAttributes>
