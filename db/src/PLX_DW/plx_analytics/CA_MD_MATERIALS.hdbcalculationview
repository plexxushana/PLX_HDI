    <calculatedMeasures>
      <measure id="COUNT_UNIQUE_MATNR" hidden="false" order="41" aggregationType="sum" measureType="simple" aggregatable="true" calculatedMeasureType="counter" datatype="INTEGER" expressionLanguage="COLUMN_ENGINE">
        <descriptions defaultDescription="Count of: unique material number"/>
        <exceptionAggregation exceptionAggregationType="countDistinct">
          <attribute attributeName="MATNR"/>
        </exceptionAggregation>
        <formula>1</formula>
      </measure>
      <measure id="COUNT_WERKS" hidden="false" order="42" aggregationType="sum" measureType="simple" aggregatable="true" calculatedMeasureType="counter" datatype="INTEGER" expressionLanguage="COLUMN_ENGINE">
        <descriptions defaultDescription="Count of: plant"/>
        <exceptionAggregation exceptionAggregationType="countDistinct">
          <attribute attributeName="MATNR"/>
        </exceptionAggregation>
        <formula>1</formula>
      </measure>
      <measure id="PERCENT_UNIQUE" hidden="false" order="43" aggregationType="sum" measureType="simple" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="12" scale="10">
        <descriptions defaultDescription="Percent of unique materials"/>
        <formula>IF(&quot;IS_FAILED_UNIQUENESS&quot; = 0, 0, 1 - (&quot;IS_FAILED_UNIQUENESS&quot;/&quot;COUNT_OF_MATERIALS&quot;) )</formula>
      </measure>
      <measure id="PERCENT_CONFORMING_DESCRIPTIONS" hidden="false" order="44" aggregationType="sum" measureType="simple" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="12" scale="10">
        <descriptions defaultDescription="Percent of conforming material descriptions"/>
        <formula>IF(&quot;IS_MAKTX_OK&quot; = 0, 0,&#xD;
&quot;IS_MAKTX_OK&quot; /  &quot;COUNT_OF_MATERIALS&quot;)</formula>
      </measure>
      <measure id="PERCENT_COMPLETE" hidden="false" order="47" aggregationType="sum" measureType="simple" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="12" scale="10">
        <descriptions defaultDescription="Percent Complete"/>
        <formula>(&quot;IS_COMPLETE&quot;) / &quot;COUNT_OF_MATERIALS&quot;</formula>
      </measure>
    </calculatedMeasures>
