        <viewAttribute id="COUNT_OF_MPN"/>
        <viewAttribute id="FIRST_WORD_MFRNR"/>
        <viewAttribute id="COUNT_OF_MFRNR"/>
        <viewAttribute id="MAKTX"/>
        <viewAttribute id="MFRNR"/>
        <viewAttribute id="MFRPN"/>
        <viewAttribute id="LAEDA"/>
        <viewAttribute id="IPRKZ"/>
        <viewAttribute id="GEWEI"/>
        <viewAttribute id="EXTWG"/>
        <viewAttribute id="ERSDA"/>
        <viewAttribute id="ERNAM"/>
        <viewAttribute id="AENAM"/>
        <viewAttribute id="MTART"/>
        <viewAttribute id="MTPOS_MARA"/>
        <viewAttribute id="PSTAT"/>
        <viewAttribute id="SLED_BBD"/>
        <viewAttribute id="VPSTA"/>
        <viewAttribute id="MEINS"/>
        <viewAttribute id="MBRSH"/>
        <viewAttribute id="IS_FAILED_UNIQUENESS"/>
        <viewAttribute id="COUNT_OF_MATERIALS"/>
        <viewAttribute id="MATNR_FORMATTED"/>
        <viewAttribute id="LVORM"/>
      </viewAttributes>
      <calculatedViewAttributes>
        <calculatedViewAttribute datatype="TINYINT" id="COUNT_OF_WORDS" expressionLanguage="COLUMN_ENGINE">
          <formula>1 + strlen(trim(&quot;MAKTX&quot;)) - strlen(replace(trim(&quot;MAKTX&quot;), ' ', ''))</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="TINYINT" id="COUNT_OF_CHARACTERS" expressionLanguage="COLUMN_ENGINE">
          <formula>strlen(trim(&quot;MAKTX&quot;))</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="TINYINT" id="IS_MFRPN_IN_MAKTX" expressionLanguage="COLUMN_ENGINE">
          <formula>if(&quot;MFRPN&quot; = '',
  0,
  if(instr(&quot;MAKTX&quot;,trim(&quot;MFRPN&quot;))>0,
  1,
  0)
  )</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="TINYINT" id="IS_SPECIAL_CHARS_IN_MAKTX" expressionLanguage="COLUMN_ENGINE">
          <formula>if( 
(instr(&quot;MAKTX&quot;,'+') > 0 or 
instr(&quot;MAKTX&quot;,'&amp;') > 0 or 
instr(&quot;MAKTX&quot;,'*') > 0 or 
instr(&quot;MAKTX&quot;,'?') > 0 or 
instr(&quot;MAKTX&quot;,'>') > 0 or 
instr(&quot;MAKTX&quot;,'&lt;') > 0 or 
instr(&quot;MAKTX&quot;,'|') > 0 or 
instr(&quot;MAKTX&quot;,'(') > 0 or 
instr(&quot;MAKTX&quot;,')') > 0 or 
instr(&quot;MAKTX&quot;,'_') > 0 or 
instr(&quot;MAKTX&quot;,'#') > 0 or 
instr(&quot;MAKTX&quot;,'@') > 0 or 
instr(&quot;MAKTX&quot;,'!') > 0 or 
instr(&quot;MAKTX&quot;,'\$') > 0 or 
instr(&quot;MAKTX&quot;,'%') > 0 or 
instr(&quot;MAKTX&quot;,'^') > 0 or 
instr(&quot;MAKTX&quot;,'=') > 0 or 
instr(&quot;MAKTX&quot;,'[') > 0 or 
instr(&quot;MAKTX&quot;,']') > 0 or 
instr(&quot;MAKTX&quot;,';') > 0 or 
instr(&quot;MAKTX&quot;,',') > 0 or 
instr(&quot;MAKTX&quot;,'~') > 0 or 
instr(&quot;MAKTX&quot;,'`') > 0 or 
instr(&quot;MAKTX&quot;,'{') > 0 or 
instr(&quot;MAKTX&quot;,'}') > 0 or 
instr(&quot;MAKTX&quot;,'&quot;') > 0), 
1, 
0)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="TINYINT" id="IS_MAKTX_OK" expressionLanguage="COLUMN_ENGINE">
          <formula>if( 
(&quot;COUNT_OF_CHARACTERS&quot;>10 and 
&quot;COUNT_OF_WORDS&quot;>2   and 
&quot;IS_MFRPN_IN_MAKTX&quot;=0 and 
&quot;IS_SPECIAL_CHARS_IN_MAKTX&quot;=0), 1, 0)</formula>
        </calculatedViewAttribute>
      </calculatedViewAttributes>
      <input node="#COMPLETE">
        <mapping xsi:type="Calculation:AttributeMapping" target="RES_DEVICE_CLASS" source="RES_DEVICE_CLASS"/>
