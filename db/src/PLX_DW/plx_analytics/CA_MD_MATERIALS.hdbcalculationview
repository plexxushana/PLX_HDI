    <calculationView xsi:type="Calculation:ProjectionView" id="UNIQUE">
      <descriptions/>
      <viewAttributes>
        <viewAttribute id="MATNR"/>
        <viewAttribute id="FIRST_WORD_MAKTX"/>
        <viewAttribute id="COUNT_OF_MAKTX"/>
        <viewAttribute id="COUNT_OF_MPN"/>
        <viewAttribute id="FIRST_WORD_MFRNR"/>
        <viewAttribute id="COUNT_OF_MFRNR"/>
        <viewAttribute id="MAKTX"/>
      </viewAttributes>
      <calculatedViewAttributes>
        <calculatedViewAttribute datatype="TINYINT" id="IS_FAILED_UNIQUENESS" expressionLanguage="COLUMN_ENGINE">
          <formula>IF(&#xD;
( ISNULL(&quot;COUNT_OF_MAKTX&quot;) AND ISNULL(&quot;COUNT_OF_MPN&quot;) )&#xD;
OR&#xD;
( ISNULL(&quot;COUNT_OF_MFRNR&quot;)AND ISNULL(&quot;COUNT_OF_MPN&quot;) )&#xD;
OR &#xD;
( ISNULL(&quot;COUNT_OF_MFRNR&quot;)AND ISNULL(&quot;COUNT_OF_MAKTX&quot;) ), 0, 1)</formula>
        </calculatedViewAttribute>
      </calculatedViewAttributes>
      <input node="#FN_MATERIAL_DUPLICATES">
        <mapping xsi:type="Calculation:AttributeMapping" target="MATNR" source="MATNR"/>
        <mapping xsi:type="Calculation:AttributeMapping" target="FIRST_WORD_MAKTX" source="FIRST_WORD_MAKTX"/>
        <mapping xsi:type="Calculation:AttributeMapping" target="COUNT_OF_MAKTX" source="COUNT_OF_MAKTX"/>
        <mapping xsi:type="Calculation:AttributeMapping" target="COUNT_OF_MPN" source="COUNT_OF_MPN"/>
        <mapping xsi:type="Calculation:AttributeMapping" target="FIRST_WORD_MFRNR" source="FIRST_WORD_MFRNR"/>
        <mapping xsi:type="Calculation:AttributeMapping" target="COUNT_OF_MFRNR" source="COUNT_OF_MFRNR"/>
        <mapping xsi:type="Calculation:AttributeMapping" target="MAKTX" source="MAKTX"/>
      </input>
    </calculationView>
    <calculationView xsi:type="Calculation:JoinView" id="DESCRIPTION" joinType="referential">
      <descriptions/>
      <viewAttributes>
        <viewAttribute id="RES_DEVICE_CLASS"/>
        <viewAttribute id="RES_DEVICE_LICENCE"/>
